{
    "name": "silverstripe/recipe-content-blocks",
    "type": "silverstripe-recipe",
    "description": "Add content blocks to your Silverstripe CMS project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "~4.10.0@stable",
        "dnadesign/silverstripe-elemental": "~4.8.0@stable",
        "silverstripe/elemental-fileblock": "~2.3.0@stable",
        "silverstripe/elemental-bannerblock": "~2.4.0@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "dnadesign/silverstripe-elemental-userforms": "Add integration logic for Elemental and Userforms"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}